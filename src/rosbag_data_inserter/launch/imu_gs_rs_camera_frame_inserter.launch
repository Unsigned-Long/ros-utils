    <node pkg="rosbag_data_inserter" type="imu_gs_rs_camera_frame_inserter" name="imu_gs_rs_camera_frame_inserter_node"
          output="screen">
        <param name="src_bag_path" value="/home/csl/dataset/HCIL-Calib/data_202332320276/lidar/lidar.bag"
               type="string"/>
        <param name="dst_bag_path" value="/home/csl/dataset/HCIL-Calib/data_202332320276/lidar_imu_camera.bag"
