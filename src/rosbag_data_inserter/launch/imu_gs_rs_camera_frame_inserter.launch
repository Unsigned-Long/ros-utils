        <param name="imu_dst_topic" value="/imu_raw_frames" type="string"/>
        <param name="imu_dst_frame_id" value="imu" type="string"/>

        <param name="gs_camera_dst_topic" value="/gs_camera_raw_frames" type="string"/>
